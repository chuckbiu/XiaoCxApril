page {
  background-color: #efefef;
}
.fix-nav {
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: left;
  color: rgba(255, 255, 255, 0.8);
  font-size: 34rpx;
  position: relative;
  top: 0;
  left: 0;
}
.fix-nav navigator {
  display: flex;
  align-items: center;
}
.fix-nav navigator text {
  color: #646464;
  font-size: 33rpx;
}
.home-order {
  width: 90%;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 20rpx;
}
.home-order .home-img {
  position: relative;
  width: 93%;
  height: 350rpx;
  margin: 0 auto;
  padding-top: 20rpx;
  padding-bottom: 15rpx;
}
.home-order .home-img image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.home-order .home-msg {
  position: absolute;
  left: 25rpx;
  bottom: 30rpx;
}
.home-order .home-msg .home-type {
  color: #fff;
  font-size: 35rpx;
  margin-bottom: 10rpx;
}
.home-order .home-msg .home-traits {
  display: flex;
}
.home-order .home-msg .home-traits .home-traits-box {
  width: 60rpx;
  height: 35rpx;
  color: #fff;
  font-size: 18rpx;
  text-align: center;
  line-height: 35rpx;
  border-radius: 8rpx;
  background-color: rgba(92, 92, 92, 0.5);
  margin-left: 10rpx;
}
.home-order .home-msg .home-traits .home-traits-box:first-child {
  margin-left: 0;
}
.home-order .home-bottom {
  width: 93%;
  height: 80rpx;
  margin: 0 auto;
  margin-top: 15rpx;
  padding-bottom: 15rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.home-order .home-bottom .home-price {
  color: #000;
  font-size: 38rpx;
  font-weight: 700;
}
.home-order .home-bottom .order-btn button {
  width: 280rpx;
  height: 75rpx;
  background-color: #69e4e9;
  color: #fff;
  font-size: 25rpx;
  line-height: 75rpx;
  border-radius: 20rpx;
}

