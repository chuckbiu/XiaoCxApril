page {
  background-color: #f2f2f2;
}
.top-box {
  position: relative;
  width: 100%;
  height: 500rpx;
  background-color: #fff;
}
.top-box .head_bg {
  width: 100%;
  height: 100%;
}
.top-box .head_bg image {
  width: 100%;
  height: 100%;
  border-radius: 0 0 60rpx 60rpx;
}
.top-box .head_bg .home-price {
  width: 200rpx;
  height: 85rpx;
  background-color: rgba(211, 205, 210, 0.9);
  position: absolute;
  bottom: -35rpx;
  right: 70rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 15rpx;
}
.top-box .head_bg .home-price text {
  color: #cf878d;
  font-size: 35rpx;
  font-weight: 700;
}
.top-box .fix-nav {
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-align: left;
  color: rgba(255, 255, 255, 0.8);
  font-size: 34rpx;
  position: fixed;
  top: 0;
  left: 0;
}
.home-msg {
  width: 100%;
  height: 170rpx;
  margin-top: -15rpx;
  background-color: #fff;
  border-radius: 0 0 60rpx 60rpx;
}
.home-msg .home-msg-box {
  width: 93%;
  background-color: #fff;
  margin: 0 auto;
}
.home-msg .home-msg-box .home-name {
  padding: 30rpx 0 20rpx 30rpx;
}
.home-msg .home-msg-box .home-name text {
  color: #000;
  font-size: 35rpx;
  font-weight: 600;
}
.home-msg .home-msg-box .home-traits {
  display: flex;
  padding: 5rpx 0 5rpx 30rpx;
}
.home-msg .home-msg-box .home-traits .home-traits-box {
  width: 100rpx;
  height: 40rpx;
  color: #fff;
  font-size: 23rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 5rpx;
  background-color: rgba(92, 92, 92, 0.5);
  margin-left: 10rpx;
}
.home-msg .home-msg-box .home-traits .home-traits-box:first-child {
  margin-left: 0;
  color: #afb0d3;
  background-color: #f5f8ff;
}
.home-msg .home-msg-box .home-traits .home-traits-box:nth-child(2) {
  color: #f7aaad;
  background-color: #fff3f3;
}
.home-msg .home-msg-box .home-traits .home-traits-box:nth-child(3) {
  color: #f9e0a5;
  background-color: #fdfbf7;
}
.home-msg .home-msg-box .home-traits .home-traits-box:nth-child(4) {
  color: #FFC0CB;
  background-color: #fdfbf7;
}
.home-describes {
  width: 100%;
  background-color: #fff;
  margin-top: 15rpx;
  border-radius: 60rpx 60rpx 0 0;
}
.home-describes .home-describes-title {
  width: 100%;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
}
.home-describes .home-describes-title text {
  color: #000;
  font-size: 38rpx;
  font-weight: 700;
}
.home-describes .home-describes-content {
  width: 90%;
  margin: 0 auto;
  margin-top: 20rpx;
}
.home-describes .home-describes-content text {
  color: #bfbec1;
  font-size: 28rpx;
}
.home-describes .home-describes-content .extend-text {
  color: #bebedd;
  font-size: 30rpx;
}
.home-describes .home-describes-facility {
  width: 90%;
  margin: 0 auto;
  margin-top: 40rpx;
}
.home-describes .home-describes-facility .home-describes-facility-title {
  width: 100%;
}
.home-describes .home-describes-facility .home-describes-facility-title text {
  color: #535353;
  font-size: 30rpx;
  font-weight: 700;
}
.home-describes .home-describes-facility .home-describes-facility {
  width: 100%;
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
.home-describes .home-describes-facility .home-describes-facility .home-describes-facility-item {
  width: 20%;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.home-describes .home-describes-facility .home-describes-facility .home-describes-facility-item image {
  width: 40rpx;
  height: 40rpx;
}
.home-describes .home-describes-facility .home-describes-facility .home-describes-facility-item text {
  color: #999999;
  font-size: 20rpx;
  font-weight: 700;
  padding-top: 10rpx;
}
.order-btn {
  margin-top: 200rpx;
  background-color: #fff;
}
.order-btn button {
  width: 80%;
  height: 90rpx;
  background-color: #69e4e9;
  border-radius: 30rpx;
  color: #fff;
  font-size: 33rpx;
  line-height: 90rpx;
}

