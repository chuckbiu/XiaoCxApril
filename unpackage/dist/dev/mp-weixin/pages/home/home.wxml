<view><view class="head_bg"><image src="https://s1.ax1x.com/2023/01/08/pSZt2Ed.png" mode="widthFix"></image></view><swiper class="swiper" autoplay="{{true}}" indicator-dots="{{true}}" indicator-color="#E9E9E9" indicator-active-color="#6de5eb" circular="{{true}}"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><swiper-item class="swiper-item"><image src="{{item.src}}" mode></image></swiper-item></block></swiper><view class="fun"><block wx:for="{{funList}}" wx:for-item="item" wx:for-index="__i1__" wx:key="title"><view data-event-opts="{{[['tap',[['gotoPage',['$0'],[[['funList','title',item.title,'title']]]]]]]}}" class="fun-box" bindtap="__e"><text class="fun-box-title">{{item.title}}</text><image class="fun-box-bg" src="{{item.imgSrc}}" mode></image></view></block></view><view class="quick-entry"><block wx:for="{{quickList}}" wx:for-item="item" wx:for-index="__i2__" wx:key="text"><view data-event-opts="{{[['tap',[['gotoEntry',['$0'],[[['quickList','text',item.text,'text']]]]]]]}}" class="entry-box" bindtap="__e"><image src="{{item.imgSrc}}"></image><text class="entry-text">{{item.text}}</text></view></block></view><view class="activity"><view class="activity-top"><text class="activity-title">活动一览</text></view><block wx:for="{{activityList}}" wx:for-item="item" wx:for-index="__i3__" wx:key="*this"><view data-event-opts="{{[['tap',[['gotoActivttyDetail',['$0'],[[['activityList','',__i3__]]]]]]]}}" class="activity-item" bindtap="__e"><image class="activity-item-image" src="{{item.imgUrl}}" mode></image><view class="activity-item-content"><text class="activity-item-content-title">{{item.title}}</text><view class="activity-item-content-bottom"><text>{{item.time}}</text><text class="activity-item-content-bottom-textright">{{item.view}}</text></view></view></view></block></view></view>