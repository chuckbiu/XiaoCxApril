<view><block wx:for="{{serviceList}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><view data-event-opts="{{[['tap',[['gotServiceDetail',['$0'],[[['serviceList','',index]]]]]]]}}" class="service-list-item" bindtap="__e"><view class="item-image"><image src="{{item.photos[0].url?item.photos[0].url:'https://s1.ax1x.com/2023/01/18/pS3RE0P.png'}}" mode="scaleToFill"></image></view><view class="item-content"><view class="item-content-title">{{item.name}}</view><block wx:if="{{item.business.rating>=4.0}}"><view class="item-content-center"><text class="center-big">{{item.business.rating}}</text><text class="center-small">推荐</text></view></block><view class="item-content-bottom">{{item.address}}</view></view></view></block><view style="width:100%;height:35px;"></view></view>